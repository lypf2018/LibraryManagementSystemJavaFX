<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.FineManageController">
   <children>
      <TableView fx:id="fineTable" layoutX="108.0" layoutY="85.0" prefHeight="331.0" prefWidth="384.0">
