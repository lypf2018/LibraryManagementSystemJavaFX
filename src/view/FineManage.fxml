          <TableColumn fx:id="cardIdColumn" prefWidth="75.0" text="Card_ID" />
          <TableColumn fx:id="totalAmountColumn" prefWidth="75.0" text="Fine_Amt" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button layoutX="417.0" layoutY="426.0" mnemonicParsing="false" onAction="#payFine" text="Pay Fine" />
      <Button layoutX="108.0" layoutY="426.0" mnemonicParsing="false" onAction="#search" text="Search" />
      <Button layoutX="247.0" layoutY="426.0" mnemonicParsing="false" onAction="#updateFines" text="Update Fines" />
   </children>
</AnchorPane>
